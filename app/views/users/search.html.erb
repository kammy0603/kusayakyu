<h1 class="title">
  検索結果
</h1>
<div class="result">
<% if @results.length !=0 %>
  <% @results.each do |result| %>
  <div class='list'>
  <%= image_tag result.avatar, class: "item-img" %>
    <td>
    <br>
    <li class="member-index">
      <div class="menmber-detail">
      <%= result.name %>
      </div>
      <div class="menmber-detail">
      <%= result.term.name %>
      </div>
      <div class="menmber-detail">
      <%= result.level.name %>
      </div>
      <div class="menmber-detail">
      <%= result.frequency.name %>
      </div>
      <div class="menmber-detail">
      <%= result.prefecture.name %>
      </div>
    </li>
  </div>
  <% end %>
<% else %>
  該当する商品はありません
<% end %>
</div>

<div class="top-back">
<%= link_to 'トップページへ戻る', root_path%>
</div>